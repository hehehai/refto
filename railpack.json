{
  "$schema": "https://schema.railpack.com",
  "provider": "node",
  "packages": {
    "node": "24.6.0",
    "pnpm": "10.14.0"
  },
  "steps": {
    "install": {
      "commands": ["pnpm install"]
    },
    "build": {
      "inputs": [{ "step": "install" }],
      "commands": ["pnpm run build"]
    }
  },
  "deploy": {
    "startCommand": "pnpm run dev:start"
  }
}
